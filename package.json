{
  "name": "@delight-rpc/http-client",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "repository": "git@github.com:delight-rpc/http-client.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --no-cache --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s prepare clean build bundle",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "clean:bundle": "rimraf dist",
    "build": "run-p build:*",
    "build:es2015": "tsc --project tsconfig.build.json --module commonjs --target es2015 --outDir lib/es2015",
    "build:es2018": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib/es2018",
    "bundle": "rollup --config rollup.config.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.1.21",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "delight-rpc": "^1.1.1",
    "eslint": "^8.2.0",
    "husky": "4",
    "jest": "^27.3.1",
    "msw": "^0.35.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.7",
    "ts-patch": "^2.0.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@blackglory/http-status": "^1.0.0",
    "extra-fetch": "^1.8.2",
    "extra-promise": "^0.19.1",
    "extra-request": "^3.0.2",
    "extra-response": "^0.3.8",
    "justypes": "^0.1.6"
  },
  "peerDependencies": {
    "delight-rpc": "^1.1.1"
  }
}
